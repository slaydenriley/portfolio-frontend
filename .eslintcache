rn(\n      <div onChange={this.handleChange} onSubmit={this.handleSubmit}>\n        <CommentInput name={this.props.user_name}/>\n      </div>\n    )\n  }\n}\n\nconst mapStateToProps = state => {\n  return {\n    user_id: state.account.user.id,\n    post_id: state.single_post.post.id,\n    user_name: state.account.user.name\n  }\n}\n\nexport default connect(mapStateToProps, {addComment})(AddCommentContainer)\n","/Users/riley/portfolio/frontend/src/components/comments/DeleteComment.js",[],"/Users/riley/portfolio/frontend/src/containers/comments/deleteCommentContainer.js",["315"],"import React from 'react'\nimport {connect} from 'react-redux'\nimport Comment from '../../components/comments/Comments'\nimport deleteComment from '../../actions/deleteComment'\n\nclass DeleteCommentContainer extends React.Component {\n\n  handleDelete = (event) => {\n    event.preventDefault()\n\n    if (window.confirm(\"Are you sure you want to delete this comment?\")) {\n      let id = event.target.id\n      let post_id = event.target.value\n      let formData = {\n        comment: {\n          id: id,\n          post_id: this.props.post_id\n        }\n      }\n      this.props.deleteComment(formData)\n    }\n  }\n\n  render() {\n    return (\n      <div>\n        <Comment delete={this.handleDelete} comments={this.props.comments} user_id={this.props.user_id}/>\n      </div>)\n  }\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    user_id: state.account.user.id,\n    post_id: state.single_post.post.id\n  }\n}\n\nexport default connect(mapStateToProps, {deleteComment})(DeleteCommentContainer)\n","/Users/riley/portfolio/frontend/src/containers/authorized/signupContainer.js",[],"/Users/riley/portfolio/frontend/src/actions/signupUser.js",[],"/Users/riley/portfolio/frontend/src/components/Signup.js",[],"/Users/riley/portfolio/frontend/src/components/Resume.js",["316"],"import resume from '../images/resumeImg.jpg'\nimport resumePdf from '../images/resumePdf.pdf'\n\nconst Resume = () => 